{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  data() {\n    return {\n      form: {\n        userName: '',\n        password: '',\n        role: ''\n      },\n      tableData: [],\n      userName: '',\n      pageNum: 1,\n      pageSize: 2,\n      total: 0,\n      dialogVisible: false\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('/user/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          userName: this.userName\n        }\n      }).then(res => {\n        if (res.code == '200') {\n          this.total = res.data.total;\n          this.tableData = res.data.records;\n        }\n      });\n    },\n    reset() {\n      this.userName = '';\n      this.load();\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    add() {\n      this.form = {};\n      this.dialogVisible = true;\n    },\n    submit() {\n      request.post('/user', this.form).then(res => {\n        if (res.code == '200') {\n          this.$message.success(\"添加成功\");\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"添加失败\");\n        }\n      });\n    },\n    update(data) {\n      this.form = data;\n      this.dialogVisible = true;\n    }\n  }\n};","map":{"version":3,"names":["request","data","form","userName","password","role","tableData","pageNum","pageSize","total","dialogVisible","created","load","methods","get","params","then","res","code","records","reset","handleSizeChange","handleCurrentChange","add","submit","post","$message","success","error","update"],"sources":["src/views/UserView.vue"],"sourcesContent":["<template>\r\n  <div class=\"user\">\r\n    <div style=\"margin-bottom: 10px\">\r\n      <el-input v-model=\"userName\" placeholder=\"请输入内容\" style=\"width: 200px;margin-right: 10px\"></el-input>\r\n      <el-button type=\"warning\" @click=\"load\">查询</el-button>\r\n      <el-button type=\"primary\" @click=\"reset\">重置</el-button>\r\n      <el-button type=\"success\" @click=\"add\">新增</el-button>\r\n    </div>\r\n      <el-table :data=\"tableData\" style=\"width: 100%;margin-bottom: 10px\">\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"userName\" label=\"姓名\" width=\"180\"></el-table-column>\r\n        <el-table-column prop=\"role\" label=\"权限\" width=\"180\"></el-table-column>\r\n      <el-table-column  label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" @click=\"update(scope.row)\">编辑</el-button>\r\n          <el-button type=\"danger\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n      </el-table>\r\n    <div>\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 4, 10, 20]\"\r\n          :page-size=\"2\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n    <el-dialog title=\"用户信息\" :visible.sync=\"dialogVisible\" width=\"30%\">\r\n      <el-form :model=\"form\" label-width=\"80px\">\r\n        <el-form-item label=\"姓名\">\r\n          <el-input v-model=\"form.userName\" placeholder=\"请输入姓名\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-input v-model=\"form.password\" placeholder=\"请输入密码\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"权限\">\r\n          <el-radio v-model=\"form.role\" label=\"管理员\"></el-radio>\r\n          <el-radio v-model=\"form.role\" label=\"用户\"></el-radio>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\">创建</el-button>\r\n          <el-button @click=\"dialogVisible\">取消</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport request from \"@/utils/request\";\r\nexport default {\r\n  data(){\r\n    return{\r\n      form:{\r\n        userName:'',\r\n        password:'',\r\n        role:''\r\n      },\r\n      tableData:[],\r\n      userName: '',\r\n      pageNum:1,\r\n      pageSize:2,\r\n      total:0,\r\n      dialogVisible:false\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods:{\r\n    load(){\r\n      request.get('/user/selectPage',{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n          userName:this.userName\r\n        }\r\n      }).then(res=>{\r\n        if (res.code == '200') {\r\n          this.total=res.data.total\r\n          this.tableData = res.data.records\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.userName =''\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize){\r\n      this.pageSize=pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    add(){\r\n      this.form = {}\r\n      this.dialogVisible= true\r\n    },\r\n    submit(){\r\n      request.post('/user',this.form).then(res=>{\r\n        if (res.code == '200') {\r\n          this.$message.success(\"添加成功\")\r\n          this.dialogVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"添加失败\")\r\n        }\r\n      })\r\n    },\r\n    update(data){\r\n      this.form=data\r\n      this.dialogVisible=true\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAmDA,OAAAA,OAAA;AACA;EACAC,KAAA;IACA;MACAC,IAAA;QACAC,QAAA;QACAC,QAAA;QACAC,IAAA;MACA;MACAC,SAAA;MACAH,QAAA;MACAI,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,aAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MA